<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<Tab xmlns:fx="http://javafx.com/fxml/1" text="Registration" xmlns="http://javafx.com/javafx/8"
     fx:controller="kassoc.controller.RegistrationController">
    <AnchorPane>
        <Accordion AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <panes>
                <TitledPane animated="true" text="BDE Registration">
                    <AnchorPane>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                            </padding>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="33"/>
                                    <ColumnConstraints hgrow="SOMETIMES"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <Label text="Name" GridPane.halignment="LEFT"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="First Name" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Mail address" GridPane.halignment="LEFT" GridPane.rowIndex="2"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Birthday" GridPane.halignment="LEFT" GridPane.rowIndex="3"
                                       GridPane.valignment="CENTER">
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="lastNameInput" GridPane.columnIndex="1" styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <TextField fx:id="firstNameInput" GridPane.columnIndex="1" GridPane.rowIndex="1"
                                           styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <TextField fx:id="mailInput" GridPane.columnIndex="1" GridPane.rowIndex="2"
                                           styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <DatePicker fx:id="birthdayInput" GridPane.columnIndex="1" GridPane.rowIndex="3"
                                            styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </DatePicker>
                            </GridPane>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <padding>
                                    <Insets top="10.0"/>
                                </padding>
                                <Button maxWidth="Infinity" mnemonicParsing="false" text="Ok"
                                        onAction="#okAction" styleClass="input">
                                </Button>
                            </GridPane>
                        </VBox>
                    </AnchorPane>
                </TitledPane>
                <TitledPane animated="true" text="BDS Registration">
                    <AnchorPane>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                            </padding>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="33.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <Label text="Sport Practice" GridPane.halignment="LEFT"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Level" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Participation tournament" GridPane.halignment="LEFT" GridPane.rowIndex="2"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <TextField fx:id="sportPracticInput" GridPane.columnIndex="1" styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <TextField fx:id="levelInput" GridPane.columnIndex="1" GridPane.rowIndex="1"
                                           styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <CheckBox fx:id="tournamentInput" GridPane.columnIndex="1" GridPane.rowIndex="2"
                                          styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </CheckBox>
                            </GridPane>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <padding>
                                    <Insets top="10.0"/>
                                </padding>
                                <Button maxWidth="Infinity" mnemonicParsing="false" text="Ok"
                                        onAction="#okBdsInput" styleClass="input">
                                </Button>
                            </GridPane>
                        </VBox>
                    </AnchorPane>
                </TitledPane>
                <TitledPane animated="true" text="ALUMNICE Registration">
                    <AnchorPane>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                            </padding>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="33"/>
                                    <ColumnConstraints hgrow="SOMETIMES"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <Label text="Business Interest" GridPane.halignment="LEFT"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Thematic Interest" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <TextField fx:id="businessInput" GridPane.columnIndex="1" styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <TextArea fx:id="thematicInput" GridPane.columnIndex="1" GridPane.rowIndex="1"
                                          styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextArea>
                            </GridPane>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <padding>
                                    <Insets top="10.0"/>
                                </padding>
                                <Button maxWidth="Infinity" mnemonicParsing="false" text="Ok"
                                        onAction="#okAluAction" styleClass="input">
                                </Button>
                            </GridPane>
                        </VBox>
                    </AnchorPane>
                </TitledPane>
                <TitledPane animated="true" text="General Registration">
                    <AnchorPane>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                            </padding>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="33"/>
                                    <ColumnConstraints hgrow="SOMETIMES"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <Label text="Phone number" GridPane.halignment="LEFT"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Address" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <Label text="Acdemic record" GridPane.halignment="LEFT" GridPane.rowIndex="2"
                                       GridPane.valignment="CENTER">
                                    <padding>
                                        <Insets right="10.0"/>
                                    </padding>
                                </Label>
                                <TextField fx:id="numberInput" GridPane.columnIndex="1" styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <TextField fx:id="addressInput" GridPane.columnIndex="1" GridPane.rowIndex="1"
                                           styleClass="input">
                                    <GridPane.margin>
                                        <Insets bottom="3.0" top="3.0"/>
                                    </GridPane.margin>
                                </TextField>
                                <Button fx:id="academicRecordInput" GridPane.columnIndex="1" GridPane.rowIndex="2"
                                        text="Open Folder" onAction="#academicRecordAction" styleClass="input">
                                </Button>
                            </GridPane>
                            <GridPane>
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" percentWidth="100.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                    <RowConstraints vgrow="SOMETIMES"/>
                                </rowConstraints>
                                <padding>
                                    <Insets top="10.0"/>
                                </padding>
                                <Button maxWidth="Infinity" mnemonicParsing="false" text="Ok"
                                        onAction="#okGeneralAction" styleClass="input">
                                </Button>
                            </GridPane>
                        </VBox>
                    </AnchorPane>
                </TitledPane>
            </panes>
        </Accordion>
    </AnchorPane>
</Tab>