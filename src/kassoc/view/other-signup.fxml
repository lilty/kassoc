<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minWidth="500" xmlns="http://javafx.com/javafx/8"
            fx:controller="kassoc.controller.InscriptionOtherController"
            stylesheets="/kassoc/style.css">
    <TitledPane fx:id="signUpPane" animated="false" text="Inscription" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
            <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
                <padding>
                    <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
                </padding>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" percentWidth="33"/>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES"/>
                        <RowConstraints vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <Label text="Phone number" GridPane.halignment="LEFT"
                           GridPane.valignment="CENTER">
                        <padding>
                            <Insets right="10.0"/>
                        </padding>
                    </Label>
                    <Label text="Address" GridPane.halignment="LEFT" GridPane.rowIndex="1"
                           GridPane.valignment="CENTER">
                        <padding>
                            <Insets right="10.0"/>
                        </padding>
                    </Label>
                    <Label text="School folder" GridPane.halignment="LEFT" GridPane.rowIndex="2"
                           GridPane.valignment="CENTER">
                        <padding>
                            <Insets right="10.0"/>
                        </padding>
                    </Label>
                    <TextField fx:id="numberInput" GridPane.columnIndex="1" styleClass="input">
                        <GridPane.margin>
                            <Insets bottom="3.0" top="3.0"/>
                        </GridPane.margin>
                    </TextField>
                    <TextField fx:id="addressInput" GridPane.columnIndex="1" GridPane.rowIndex="1"
                               styleClass="input">
                        <GridPane.margin>
                            <Insets bottom="3.0" top="3.0"/>
                        </GridPane.margin>
                    </TextField>
                    <Button fx:id="schoolFolderInput" GridPane.columnIndex="1" GridPane.rowIndex="2"
                               styleClass="input">
                        <GridPane.margin>
                            <Insets bottom="3.0" top="3.0"/>
                        </GridPane.margin>
                    </Button>
                </GridPane>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0"/>
                        <ColumnConstraints hgrow="SOMETIMES"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints vgrow="SOMETIMES"/>
                        <RowConstraints vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <padding>
                        <Insets top="10.0"/>
                    </padding>
                </GridPane>
            </VBox>
        </AnchorPane>
    </TitledPane>
</AnchorPane>
